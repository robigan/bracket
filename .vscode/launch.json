    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Backend",
            "type": "debugpy",
            "request": "launch",
            "module": "gunicorn",
            "args": [
                "-k",
                "bracket.uvicorn.RestartableUvicornWorker",
                "bracket.app:app",
                "--bind",
                "localhost:8400",
                "--workers",
                "1",
                "--reload"
            ],
            "jinja": true,
            "cwd": "${workspaceFolder}/backend"
        }
    ]
}